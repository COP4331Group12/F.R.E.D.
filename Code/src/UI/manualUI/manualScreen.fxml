<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSlider?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/primaryTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.manualUI.manualScreenController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
             <VBox alignment="CENTER" spacing="30" BorderPane.alignment="BOTTOM_CENTER">
                 <Button fx:id="upButton" mnemonicParsing="false" prefHeight="170.0" prefWidth="132.0" styleClass="forward-arrow-button" text="Go" onAction="#goForward" BorderPane.alignment="CENTER" />
                 <Button fx:id="backButton" mnemonicParsing="false" prefHeight="167.0" prefWidth="129.0" styleClass="backward-arrow-button" text="Back" onAction="#goBackward" BorderPane.alignment="CENTER" />
               <JFXSlider fx:id="turnSlider" max="90" min="-90" scaleX="2" scaleY="2" maxWidth="500" minHeight="160" showTickLabels="true" showTickMarks="true" value="0">
                  <padding>
                     <Insets left="40.0" right="40.0" top="60.0" />
                  </padding>
               </JFXSlider>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <BorderPane.margin>
                  <Insets top="200.0" />
               </BorderPane.margin>
             </VBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
